{
    "image": "mcr.microsoft.com/vscode/devcontainers/go:1.18",
    "extensions": [
        "golang.go"
    ],
    "features": {
        "github-cli": "latest",
        "aws-cli": "latest"
    },
    "mounts": [
        "source=${localEnv:HOME}/.aws,target=/root/.aws,type=bind,consistency=cached"
    ]
}