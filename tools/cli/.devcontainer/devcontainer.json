{
    "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:16",
    "features": {
        "github-cli": "latest",
        "aws-cli": "latest",
        "docker-in-docker": {
            "version": "latest"
        }
    },
    "mounts": [
        "source=${localEnv:HOME}/.aws,target=/root/.aws,type=bind,consistency=cached"
    ]
}